{
  "name": "sanity-template-remix-clean",
  "private": true,
  "scripts": {
"dev": "pnpm -r dev & sass --watch app/styles/main.scss:app/styles/main.css",
  "format": "prettier --cache --write ."
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.3"
  },
  "packageManager": "pnpm@9.12.2",
  "dependencies": {
    "sass": "^1.80.4"
  }
}
